{
  "name": "ES6 Repl",
  "version": "0.0.1",
  "description": "ES6 transpiler based on Traceur",
  "devtools_page": "es6-repl.html",
  "manifest_version": 2,
  "icons": {
    "128": "128.png"
  },
  "background": {
    "scripts": ["background.js"]
  },
  "content_scripts": [{
    "matches": ["http://*/*", "https://*/*"],
    "js": [
      "traceur.js",
      "content.js"
    ]
  }],
  "permissions": [
    "<all_urls>",
    "storage",
    "tabs"
  ],
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
}
